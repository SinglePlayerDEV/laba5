#include <iostream>
#include <stdlib.h>
using namespace std;
const int n = 3;
const int m = 3;
int summa(int a[][m])
{
    int i, j, s = 0;
    for (i = 0; i < m; i++)
        for (j = 0; j < m; j++)
            if (i > j) s += a[i][j];
    return s;
}
int main() {
    setlocale(LC_ALL, "ru");
    int k, a[m][m], i, j, sum[n], max;
    for (k = 0; k < n; k++)
    {
        for (i = 0; i < m; i++)
        {
            for (j = 0; j < m; j++)
            {
                a[i][j] = rand() % 50;
                cout << a[i][j] << "\t";
            }
            cout << endl;
        }
        sum[k] = summa(a);
        cout << sum[k] << endl;
    }
    max = sum[0];
    for (i = 1; i < n; i++)
        if (sum[i] > max) max = sum[i];
    cout << "Максимальная сумма равна " << max << endl;
    return 0;
}
